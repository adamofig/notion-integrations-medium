{
  "name": "notionintegrations",
  "version": "0.0.1",
  "description": "integrate usefull notion events with medium and other platforms",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "npx tsc -p tsconfig.cloud.json",
    "start": "npx tsc & node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q --inspect dist/index.js\"",
    "deploy-function": "gcloud functions deploy app --trigger-http --allow-unauthenticated --runtime nodejs20"
  },
  "author": "adamofig",
  "license": "ISC",
  "dependencies": {
    "@notionhq/client": "^2.2.13",
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "notion-page-to-html": "^1.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.9",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2"
  }
}
